{
  "name": "angular-migration-tracker",
  "version": "1.0.0",
  "description": "Track AngularJS to React migration progress for Portainer",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "tsc -b && vite build",
    "build:results": "npm run analyze:local && cp results.json public/results.json",
    "preview": "vite preview",
    "analyze": "tsx analyze.ts",
    "analyze:local": "tsx analyze.ts local",
    "analyze:github": "tsx analyze.ts github"
  },
  "packageManager": "pnpm@10.17.1",
  "dependencies": {
    "@octokit/rest": "^21.0.2",
    "@tailwindcss/vite": "^4.1.16",
    "react": "^19.1.1",
    "react-dom": "^19.1.1",
    "tailwindcss": "^4.1.16"
  },
  "devDependencies": {
    "@types/node": "^24.6.0",
    "@types/react": "^19.1.16",
    "@types/react-dom": "^19.1.9",
    "@vitejs/plugin-react": "^5.0.4",
    "tsx": "^4.19.2",
    "typescript": "~5.9.3",
    "vite": "^7.1.7"
  }
}
